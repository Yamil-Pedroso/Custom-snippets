{
  "compilerOptions": {
    "types": ["node"],
    "target": "es2020",
    "module": "CommonJS",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude":[
    "./node_modules" , "./dist"
  ]
}
